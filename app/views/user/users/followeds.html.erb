<div class='container'>
  <div class='row'>
    <div class="col-lg-8 col-md-10 col-sm-12 mx-auto mt-5">
      <h2 class="title"><%= @user.nickname %>さんのフォロワー</h2>
      <% if @user.followeds.count == 0 %>
        <p>ユーザーはいません</p>
      <% else %>
        <%= render 'follow',users: @users %>
      <% end %>
    </div>
  </div>
</div>